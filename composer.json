{
    "name": "yangweijie/think-trace",
    "description": "thinkphp debug trace",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "liu21st",
            "email": "liu21st@gmail.com"
        },
        {
            "name": "yangweijie",
            "email": "917647288@qq.com"
        }
    ],
    "require": {
    	"php": ">=7.4.0",
        "symfony/polyfill-php80": "^1.16",
        "topthink/framework": "^6.0 | ^6.1 |^8.0",
        "yangweijie/think-editor": "*"
    },
    "conflict": {
        "topthink/think-trace": "*"
    },
    "autoload": {
        "psr-4": {
            "think\\trace\\": "src"
        }
    },
    "extra": {
        "think":{
            "services":[
                "think\\trace\\Service"
            ],
            "config":{
                "trace": "src/config.php"
            }
        }
    },    
    "minimum-stability": "dev"
}
